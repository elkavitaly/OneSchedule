  },

  "DatabaseSettings": {
    "ConnectionString": "<Placeholder>",
    "DatabaseName": "<Placeholder>"
  },

  "Quartz": {
    "quartz.scheduler.instanceName": "Quartz ASP.NET Core Sample Scheduler"
  }
}
