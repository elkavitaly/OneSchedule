{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },

  "AllowedHosts": "*",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },

  "TelegramSettings": {
    "ApiKey": "<Placeholder>",
    "TestUri": "<Placeholder>"
  },

  "DatabaseSettings": {
    "ConnectionString": "<Placeholder>",
    "DatabaseName": "<Placeholder>"
  },

  "Logger": {
    "ConnectionString": "<Placeholder>"
  },

  "Quartz": {
    "quartz.scheduler.instanceName": "Quartz ASP.NET Core Sample Scheduler"
  },

  "WebHookSettings": {
    "IsEnabled": true,
    "Uri": "https://oneshedule.azurewebsites.net/api/update"
  } 
} 
